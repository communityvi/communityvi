{
	"name": "communityvi-frontend",
	"description": "Communityvi Frontend",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"watch": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"lint": "prettier --check . && eslint . && echo 'ESLint OK!' && svelte-check --fail-on-warnings && echo 'Svelte validate OK!'",
		"lint:fix": "prettier -w . && eslint . --fix && echo 'ESLint Fixed!'",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:watch": "vitest watch",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@eslint/compat": "^1.1.1",
		"@eslint/eslintrc": "^3.1.0",
		"@eslint/js": "^9.8.0",
		"@faker-js/faker": "^9.0.0",
		"@fortawesome/fontawesome-free": "^6.0.0",
		"@sveltejs/adapter-static": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@testing-library/svelte": "^5.0.0",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"@vitest/coverage-v8": "^2.0.0",
		"bulma": "^1.0.0",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.39.0",
		"globals": "^15.9.0",
		"jsdom": "^25.0.0",
		"prettier": "~3.4.0",
		"prettier-plugin-svelte": "^3",
		"sass": "^1.49",
		"svelte": "^4.0.0",
		"svelte-check": "^3.0.0",
		"svelte-eslint-parser": "^0.43.0",
		"tslib": "^2.3",
		"typescript": "^5.0.0",
		"vite": "^5.0.0",
		"vitest": "^2.0.0",
		"vitest-mock-extended": "^2.0.0"
	}
}
